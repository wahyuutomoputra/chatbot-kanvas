{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 60,
            "memory": 3008
        }
    },
    "env": {
        "TRANSFORMERS_CACHE": "/tmp/.cache"
    }
}
